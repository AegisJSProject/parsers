{
  "name": "@aegisjsproject/parsers",
  "version": "0.1.5",
  "description": "A collection of secure & minimal parsers for HTML, CSS, SVG, MathML, XML, and JSON",
  "keywords": [
    "aegis",
    "tagged-template-literals",
    "sanitizer",
    "sanitizer-api"
  ],
  "type": "module",
  "main": "./parsers.cjs",
  "module": "./parsers.js",
  "unpkg": "./bundle.js",
  "exports": {
    ".": {
      "import": "./parsers.js",
      "require": "./parsers.cjs"
    },
    "./*.js": {
      "import": "./*.js",
      "require": "./*.cjs"
    },
    "./*.mjs": {
      "import": "./*.js",
      "require": "./*.cjs"
    },
    "./*.cjs": {
      "import": "./*.js",
      "require": "./*.cjs"
    },
    "./*": {
      "import": "./*.js",
      "require": "./*.cjs"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "private": false,
  "config": {
    "serve": {
      "domain": "localhost",
      "path": "./",
      "port": 8080
    }
  },
  "scripts": {
    "test": "npm run lint:js && npm run build",
    "start": "npx @shgysk8zer0/http-server -c ./http.config.js",
    "preversion": "npm test",
    "prepare": "npm run build",
    "lint:js": "eslint .",
    "fix:js": "eslint . --fix",
    "build": "npm run build:js",
    "clean": "rm -f *.cjs *.mjs  *.map",
    "build:js": "npm run clean && rollup -c rollup.config.js",
    "create:lock": "npm i --package-lock-only --ignore-scripts --no-audit --no-fund",
    "version:bump": "npm run version:bump:patch",
    "version:bump:patch": "npm version --no-git-tag-version patch && npm run create:lock",
    "version:bump:minor": "npm version --no-git-tag-version minor && npm run create:lock",
    "version:bump:major": "npm version --no-git-tag-version major && npm run create:lock"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AegisJSProject/parsers.git"
  },
  "author": "Chris Zuber <admin@kernvalley.us>",
  "license": "MIT",
  "funding": [
    {
      "type": "librepay",
      "url": "https://liberapay.com/shgysk8zer0"
    },
    {
      "type": "github",
      "url": "https://github.com/sponsors/shgysk8zer0"
    }
  ],
  "bugs": {
    "url": "https://github.com/AegisJSProject/parsers/issues"
  },
  "homepage": "https://github.com/AegisJSProject/parsers#readme",
  "devDependencies": {
    "@aegisjsproject/dev-server": "^1.0.5",
    "@aegisjsproject/http-utils": "^1.0.4",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@shgysk8zer0/eslint-config": "^1.0.1",
    "@shgysk8zer0/http-server": "^1.1.1",
    "@shgysk8zer0/importmap": "^1.7.5",
    "eslint": "^10.0.0",
    "rollup": "^4.13.1"
  },
  "dependencies": {
    "@aegisjsproject/sanitizer": "^0.2.4",
    "@aegisjsproject/url": "^1.0.3"
  }
}
